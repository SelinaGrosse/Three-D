@article{ ISI:000082836500021,
Author = {Nagpal, AK and Purohit, R and Kiradoo, BD and Sahani, MS},
Title = {{Studies on stall-feeding of groundnut fodder vis-a-vis 3-tier
   silvi-pasture grazing based in arid ecosystem}},
Journal = {{JOURNAL OF CAMEL PRACTICE AND RESEARCH}},
Year = {{1999}},
Volume = {{6}},
Number = {{1}},
Pages = {{93-96}},
Month = {{JUN}},
Abstract = {{Twenty three camel calves (1.5-3.0 years) we re divided into 2 groups.
   Group 1 of 6 camel calves (3 males, 3 females) was stall-fed on ad
   libitum ration of dry groundnut (Arachis hypogea) guna/chara while group
   II of 17 camel calves (10 males and 7 females) sent for grazing in
   3-tier silvi-pasture for 7 hours a day. The average daily gain (g/d) was
   significantly (p<0.01) higher in group (0.427+/-0.03) than in group II
   (0.237+/-0.03). The nutritive value of groundnut guna was 2.89\% DCP and
   8.72 MJME/kg DM and that of groundnut chara was 3.78\% DCP and 8.60
   MJME/kg DM in stall-fed group. Both Groundnut guna and chara had low Ca
   contents and thus its GIT absorption was less (-27.74 and -0.97\%).
   Plant vigour and chemical composition of groundnut crop at different
   growth stages revealed continuous increase in per cent units of DM
   (11.67), CF (4.00) and NFE (4.39) with concomitant decrease in per cent
   units of CP (6.36), EE (1.88) and P(0.17). The annual sustainability of
   groundnut crop from 11.25 hectares was worked out to be 0.35 ACU (adult
   camel unit of 500 kg body weight). Cultivation of groundnut crop
   resulted in depletion of soil nutrients of available phosphate (29.69\%)
   and potash (20.75\%) but increase in per cent carbon (+454.90\%). Plant
   biodiversity of 3-tier system consisted of 51.4\% edible and 48.6\%
   non-edible species besides ground cover of various grass species. The
   results indicated that although growth was higher in stall feeding
   system, better camel growth can be achieved from 3-tier system by way of
   plant biomass enrichment through introduction of better bushes/shrubs,
   trees, grasses and supplementation.}},
Publisher = {{CAMEL PUBLISHING HOUSE}},
Address = {{67 GANDHI NAGAR WEST, NEAR LALGARH PALACE, BIKANER 334001, INDIA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Nagpal, AK (Reprint Author), Natl Res Ctr Camel, Post Bag 07, Bikaner 334001, Rajasthan, India.
   Natl Res Ctr Camel, Bikaner 334001, Rajasthan, India.}},
