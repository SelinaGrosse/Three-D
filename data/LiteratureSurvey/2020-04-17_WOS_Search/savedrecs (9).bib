@article{ ISI:000079095800009,
Author = {Zhang, W},
Title = {{Changes in species diversity and canopy cover in steppe vegetation in
   Inner Mongolia under protection from grazing}},
Journal = {{BIODIVERSITY AND CONSERVATION}},
