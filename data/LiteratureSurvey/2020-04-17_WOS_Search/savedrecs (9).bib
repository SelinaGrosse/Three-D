@article{ ISI:000084056200006,
Author = {Diaz, S and Cabido, M and Zak, M and Carretero, EM and Aranibar, J},
Title = {{Plant functional traits, ecosystem structure and land-use history along
   a climatic gradient in central-western Argentina}},
Journal = {{JOURNAL OF VEGETATION SCIENCE}},
