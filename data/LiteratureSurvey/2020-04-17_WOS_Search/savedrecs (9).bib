@article{ ISI:A1996XZ61700017,
Author = {Ouhammou, A and Muller, S and MHirit, O},
Title = {{Grazing activities impact on floristic biodiversity in thurifer juniper
   stand bordering the Toubkal National Park, High Atlas of Marrakesh,
   Morocco}},
Journal = {{ACTA BOTANICA GALLICA}},
Year = {{1996}},
Volume = {{143}},
Number = {{4-5}},
Pages = {{393-401}},
Abstract = {{The grazing activities' effect on the biodiversity was analysed in a
   high Mediterranean coniferous stand (High Atlas of Marrakesh, Morocco),
   from 1989 to 1995. The comparison of the behaviour of vegetation along
   some permanent transects localised in ungrazed and grazed areas was
   established. The study of these two areas having opposite evolutionary
   vegetation structure permitted the evaluation of the grazing effects on
   the vegetation structure by computing the specific richness,
   determination of the Shannon-Weaver diversity index, the evenness and
   life form. The results showed that the value of parameters was maximum
   four years after stopping the grazing. Whereas, the biodiversity
   decreased in the grazing area. This study shows that it is necessary to
   find a compromise in the land - use between biodiversity conservation
   and grazing.}},
