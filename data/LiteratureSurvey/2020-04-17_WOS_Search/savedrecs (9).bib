@article{ ISI:A1996XZ61700008,
Author = {Fleury, P},
Title = {{Different components of biodiversity in pastures. Examples in French
   Northern Alps}},
Journal = {{ACTA BOTANICA GALLICA}},
Year = {{1996}},
Volume = {{143}},
Number = {{4-5}},
Pages = {{291-298}},
Abstract = {{We study different components of biodiversity in permanent pastures of
   the French Northern Alps : intraspecific diversity, species diversity
   and functional diversity (adaptative strategies). These different
   components don't vary in relation to the same environmental conditions
   (physic:al environment and farming pratices), but all have an interest
   and need gestion and conservation actions. The results show that it is
   necessary to develop researchs, and then management measures, to enter
   upon biodiversity not only in terms of favorable farming practices at
   the field level (fertilisation, cutting time) but with more complexity,
   in terms of diversity of the factors of variation regarding pasture
   vegetation : physical conditions, farming practices, landscape
   structure.}},
