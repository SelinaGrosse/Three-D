@article{ ISI:A1992JE77200007,
Author = {GEORGE, TL and FOWLER, AC and KNIGHT, RL and MCEWEN, LC},
Title = {{IMPACTS OF A SEVERE DROUGHT ON GRASSLAND BIRDS IN WESTERN NORTH-DAKOTA}},
