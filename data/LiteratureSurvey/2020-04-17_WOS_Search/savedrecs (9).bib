@article{ ISI:A1995QW03900009,
Author = {NELSON, JG},
Title = {{THE NEW-FOREST, ENGLAND - A THREATENED LANDSCAPE OF GLOBAL SIGNIFICANCE}},
Journal = {{NATURAL AREAS JOURNAL}},
Year = {{1995}},
Volume = {{15}},
Number = {{2}},
Pages = {{168-174}},
Month = {{APR}},
Abstract = {{The New Forest of southeastern England is an outstanding landscape in
   both natural and cultural terms. Its scenic and other special qualities
   have evolved as a result of the interaction of soils, vegetation,
   animals, land use, and human activities over many centuries. The
   qualities of the New Forest are currently threatened with changes
   arising primarily from recreational and tourism pressures from the city
   of London and other nearby areas. The conservation and land-use policies
   and practices for the New Forest are undergoing review; various
   proposals for management changes may decrease the influence of
   institutions such as the Verderers, a Crown body that for centuries has
   played an important role in guiding land uses such as grazing by ponies
   and other livestock. This grazing has strongly influenced the unique
   character of the heath, grassland, and deciduous woodlands of the New
   Forest and contributed to a high level of biodiversity and other values
   in the area. The New Forest planning proposals therefore deserve very
   careful scrutiny before implementation. The European and global
   significance of the New Forest also merit greater recognition through
   designation, preferably as a `'world heritage site,'' which would
   encourage standards of management appropriate to its international
   stature.}},
