@article{ ISI:A1996XZ61700013,
Author = {Ouhammou, A},
Title = {{Biodiversity and management of meadow ecosystems within Toubkal National
   Park, Morocco. First results}},
Journal = {{ACTA BOTANICA GALLICA}},
Year = {{1996}},
Volume = {{143}},
Number = {{4-5}},
Pages = {{349-352}},
Abstract = {{The study presented here corresponds to our first results about
   floristic inventory of the meadows within Toubkal National Park, High
   Atlas of Marrakesh, Morocco. We attempt to show the importance of
   vegetation biodiversity. These meadows have important potentialities on
   fodder crops, Their management is traditional with two or three cuttings
   per year and fertilization by manure. These meadows are vulnerable.
   Human and animal pressure combined with flood effects threaten the
   survival of these meadows.}},
