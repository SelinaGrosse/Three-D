@article{ ISI:000222367100001,
Author = {Rook, AJ and Dumont, B and Isselstein, J and Osoro, K and WallisDeVries,
   MF and Parente, G and Mills, J},
Title = {{Matching type of livestock to desired biodiversity outcomes in pastures
   - a review}},
