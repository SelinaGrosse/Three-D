@article{ ISI:000281107500019,
Author = {Lin, Yang and Hong, Mei and Han, Guodong and Zhao, Mengli and Bai,
   Yongfei and Chang, Scott X.},
Title = {{Grazing intensity affected spatial patterns of vegetation and soil
   fertility in a desert steppe}},
