@article{ ISI:000270868700017,
Author = {Macagno, Anna Lorenza Maria and Palestrini, Claudia},
Title = {{The maintenance of extensively exploited pastures within the Alpine
   mountain belt: implications for dung beetle conservation (Coleoptera:
   Scarabaeoidea)}},
Journal = {{BIODIVERSITY AND CONSERVATION}},
Year = {{2009}},
Volume = {{18}},
Number = {{12}},
Pages = {{3309-3323}},
Month = {{NOV}},
Abstract = {{The abandonment of marginal areas and the decline of traditional
   small-scale cattle-breeding is bringing about substantial changes in
   many areas of the Alpine mountain belt. Extensively exploited pastures,
   which were colonized by typical ecological communities, are now being
   replaced by shrublands and forests. In this paper, we evaluate the
   effectiveness of the maintenance of small pastures for dung beetle
   (Scarabaeoidea: Geotrupidae, Aphodiidae, Scarabaeidae) conservation in a
   protected area of the north-western Italian Alps. From July until
   September 2007, we studied the dung beetle coenosis in the main habitats
   of the mountain belt: beech forest, scots pine forest, mountain pine
   forest and pasture. Twenty-three species were sampled. We compared
   abundance, species richness, alpha-diversity, evenness and species
   turnover between the four habitat assemblages and performed a
   correspondence analysis based on dominance patterns of the three
   families. Two main ensembles were identified: a pasture ensemble, highly
   diverse and dominated by Scarabaeidae, and a closed habitats ensemble,
   less structured and dominated by Aphodiidae and Geotrupidae. Combining
   habitat specificity and fidelity, the IndVal method showed that half of
   the species collected made a strong habitat selection toward pasture,
   while only C. granarius (Aphodiidae) was a reliable indicator of woody
   areas. We therefore conclude that the loss of extensively exploited
   pastures may bring about fundamental changes in dung insect communities.
   In protected areas, a management policy intended to preserve the
   traditional, sustainable human activities within the mountain belt is
   highly recommended for the conservation of these insects.}},
