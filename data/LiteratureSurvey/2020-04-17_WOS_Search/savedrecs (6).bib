@article{ ISI:000261067700005,
Author = {Koper, Nicola and Henderson, Darcy C. and Wilmshurst, John F. and
   Fargey, Patrick J. and Sissons, Robert A.},
Title = {{Design and Analysis of Rangeland Experiments Along Continuous Gradients}},
