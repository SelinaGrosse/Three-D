@article{ ISI:000277475000005,
Author = {Rosa Garcia, Rocio and Jose Ocharan, Francisco and Garcia, Urcesino and
   Osoro, Koldo and Celaya, Rafael},
Title = {{Arthropod fauna on grassland-heathland associations under different
   grazing managements with domestic ruminants}},
