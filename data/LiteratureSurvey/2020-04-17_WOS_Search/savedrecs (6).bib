@article{ ISI:000275946400031,
Author = {Toeroek, Peter and Deak, Balazs and Vida, Eniko and Valko, Orsolya and
   Lengyel, Szabolcs and Tothmeresz, Bela},
Title = {{Restoring grassland biodiversity: Sowing low-diversity seed mixtures can
   lead to rapid favourable changes}},
Journal = {{BIOLOGICAL CONSERVATION}},
Year = {{2010}},
Volume = {{143}},
Number = {{3}},
Pages = {{806-812}},
Month = {{MAR}},
Abstract = {{We studied the effect of sowing low-diversity seed mixtures (containing
   2-3 competitive grass species) on the regeneration of vegetation on
   croplands previously used as alfalfa fields. In four permanent plots in
   10 restored fields (four with alkali and six with loess seed mixture)
   the cover of flowering plants was recorded and phytomass was measured
   between 2006 and 2008. We asked three questions: (i) How fast will
   weedy, short-lived species decrease in abundance during secondary
   succession enhanced by sowing low-diversity seed mixtures? (ii) Can
   weeds be suppressed by sowing competitive native grasses, followed up by
   management by mowing? (iii) Can succession towards the target native
   grasslands be accelerated by sowing only low-diversity seed mixtures
   compared to set-aside old field succession? Our results showed that in
   just two years a vegetation dominated by perennial grasses has
   developed, which successfully prevented the establishment of weed
   species. These results suggested that sowing seeds of two or three
   competitive grass species is an effective tool to eliminate weed
   domination and to lead towards the restoration of species poor
   grasslands with grass domination such as alkali grasslands. However, the
   developed dense perennial grass cover and the accumulated litter may
   hamper the immigration of specialist species characteristic to reference
   grasslands. Therefore, the restoration of species-rich grasslands
   requires the facilitation of the immigration of grassland specialist
   species by further management (grazing, mowing and/or hay-transport).
   (C) 2009 Elsevier Ltd. All rights reserved.}},
