@article{ ISI:000270748300006,
Author = {Kardol, Paul and Newton, Jeffrey S. and Bezemer, T. Martijn and Maraun,
   Mark and van der Putten, Wim H.},
Title = {{Contrasting diversity patterns of soil mites and nematodes in secondary
   succession}},
