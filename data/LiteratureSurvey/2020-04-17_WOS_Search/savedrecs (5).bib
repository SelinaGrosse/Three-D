@article{ ISI:000287359200012,
Author = {Dover, J. W. and Spencer, S. and Collins, S. and Hadjigeorgiou, I. and
   Rescia, A.},
Title = {{Grassland butterflies and low intensity farming in Europe}},
Journal = {{JOURNAL OF INSECT CONSERVATION}},
Year = {{2011}},
Volume = {{15}},
Number = {{1-2, SI}},
Pages = {{129-137}},
Month = {{APR}},
Abstract = {{In this paper we describe the impact of the abandonment of traditional
   farming practices on butterflies and their habitats in traditional,
   often montane, pastoral systems. We link these declines to socioeconomic
   factors: illustrating how the failure of the CAP to support traditional
   farming leads to structural changes in farming enterprises-features
   which may be obscured by crude statistics on stock. We then call for the
   scheduled CAP reforms in 2013 to be radically realigned to support
   rather than destroy biodiversity so that any new EU agri-biodiversity
   commitments have an effective funding stream to support them.}},
