@article{ ISI:000307145300002,
Author = {Bretzel, Francesca and Malorgio, Fernando and Paoletti, Lucia and
   Pezzarossa, Beatrice},
Title = {{Response of sowed, flowering herbaceous communities suitable for
   anthropic Mediterranean areas under different mowing regimes}},
Journal = {{LANDSCAPE AND URBAN PLANNING}},
