@article{ ISI:000304757400010,
Author = {Pokorny, Nella and Fesl, Christian and Schagerl, Michael and Waringer,
   Johann},
Title = {{Factors structuring macroinvertebrate communities in a headwater stream
   and its springs in the Upper Austrian Limestone Alps}},
Journal = {{RIVER RESEARCH AND APPLICATIONS}},
