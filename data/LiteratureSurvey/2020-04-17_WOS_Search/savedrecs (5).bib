@article{ ISI:000309569600001,
Author = {Orr, Robert J. and Tozer, Katherine N. and Griffith, Bruce A. and
   Champion, Robert A. and Cook, James E. and Rutter, Steven Mark},
Title = {{Foraging paths through vegetation patches for beef cattle in
   semi-natural pastures}},
Journal = {{APPLIED ANIMAL BEHAVIOUR SCIENCE}},
