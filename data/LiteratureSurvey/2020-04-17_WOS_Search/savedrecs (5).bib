@article{ ISI:000298610500014,
Author = {Yakutin, M. V. and Andrievskii, V. S.},
Title = {{Effect of Grazing Pressure on the Complex of Destructors in Soils of Dry
   Steppes of Southern Tyva}},
Journal = {{CONTEMPORARY PROBLEMS OF ECOLOGY}},
Year = {{2011}},
Volume = {{4}},
Number = {{5}},
Pages = {{535-539}},
Month = {{OCT}},
Abstract = {{It is shown that an increase in grazing pressure in soils of dry steppes
   in Southern Tyva leads to an increase in the content of microbial
   biomass, a decrease in its specific activity, and a reduction in the
   numbers and species richness of oribatid mites. During pasture
   restoration for three years, the content of microbial biomass has
   decreased and its specific activity has increased, but the numbers and
   species richness of the oribatid mites community has not recovered.}},
