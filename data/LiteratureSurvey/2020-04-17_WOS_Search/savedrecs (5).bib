@article{ ISI:000300205600008,
Author = {Gregor, Thomas and Boensel, Dirk and Starke-Ottich, Indra and Zizka,
   Georg},
Title = {{Drivers of floristic change in large cities - A case study of
   Frankfurt/Main (Germany)}},
Journal = {{LANDSCAPE AND URBAN PLANNING}},
