@article{ ISI:000296270200004,
Author = {Hyvonen, Terho and Huusela-Veistola, Erja},
Title = {{Impact of seed mixture and mowing on food abundance for farmland birds
   in set-asides}},
Journal = {{AGRICULTURE ECOSYSTEMS \& ENVIRONMENT}},
Year = {{2011}},
Volume = {{143}},
Number = {{1, SI}},
Pages = {{20-27}},
