@article{ ISI:000304447200006,
Author = {Celis-Murillo, Antonio and Deppe, Jill L. and Ward, Michael P.},
Title = {{Effectiveness and utility of acoustic recordings for surveying tropical
   birds}},
Journal = {{JOURNAL OF FIELD ORNITHOLOGY}},
Year = {{2012}},
Volume = {{83}},
