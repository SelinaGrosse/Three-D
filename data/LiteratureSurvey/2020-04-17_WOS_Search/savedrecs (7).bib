@article{ ISI:000256597600016,
Author = {Luevano, Jaime and Mellink, Eric and Riojas-Lopez, Monica E. and
   Flores-Flores, Jose Luis},
Title = {{Nocturnal rodents at the edge of a microphyllous shrubland and
   gypsophilous grassland in San Luis Potosi, Mexico}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2008}},
Volume = {{79}},
Number = {{1}},
Pages = {{197-203}},
Month = {{JUN}},
Abstract = {{In this study we compared abundance and species richness of nocturnal
   rodents at the edge of gypsophilous grassland on an exposed gypsum
   substrate and 3 sites of microphilous shrubland, in northern San Luis
   Potosi, Mexico. We sampled at sites that differed in edaphic
   characteristics and vegetation structure, in the wet and dry seasons of
   2003 and 2004. As a population estimator we used the number of
   individuals captured in each sampling period. We captured 414
   individuals of 12 species of Heteromyidae and Muridae. We concluded that
   1) rodent communities were different among sites as a function of
   vegetation structure, and varied notably between grassland and adjacent
   shrublands: 2) Such communities reflected the vegetation mosaic more
   than season of the year: 3) the three shrubland types maintained their
   identity throughout the year; y 4) low precipitation and/or grazing,
   through the reduction of plant cover, on gypsophilous soils afected the
   rodents.}},
