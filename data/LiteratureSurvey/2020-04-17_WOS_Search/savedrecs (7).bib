@article{ ISI:000256384700007,
Author = {Wang, D. and Ba, L.},
Title = {{Ecology of meadow steppe in northeast China}},
Journal = {{RANGELAND JOURNAL}},
Year = {{2008}},
Volume = {{30}},
Number = {{2}},
Pages = {{247-254}},
Abstract = {{Native grassland in China is mostly meadow, typical or desert steppe and
   comprises 400 million hectares, similar to 40\% of the land area. We
   review past research on the meadow steppe of north-east China. Our foci
   are plant adaptation to climate, edaphic-related and defoliation
   stresses, vegetation production, grassland management, herbivore
   foraging behaviour, safe stocking rates, plant-animal interactions,
   ecosystem functioning, conservation of biodiversity and the influence of
   climate change on grassland function. Recent studies have provided some
   insights into ecological processes and functioning of meadow steppe, and
   have enabled better identification of research opportunities. Key areas
   identified for future research include plant adaptation, grassland
   function and value, monitoring of range health, ecological consequences
   of climate change on biodiversity and ecosystem function.}},
