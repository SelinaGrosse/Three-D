@article{ ISI:000243976000011,
Author = {Chazdon, Robin L. and Letcher, Susan G. and van Breugel, Michiel and
   Martinez-Ramos, Miguel and Bongers, Frans and Finegan, Bryan},
Title = {{Rates of change in tree communities of secondary Neotropical forests
   following major disturbances}},
