@article{ ISI:000207641800012,
Author = {Friend, M. A. and Robertson, S. and Masters, D. and Avery, A.},
Title = {{EverGraze - a project to achieve profit and environmental outcomes in
   the Australian grazing industries}},
Journal = {{JOURNAL OF ANIMAL AND FEED SCIENCES}},
Year = {{2007}},
Volume = {{16}},
Number = {{2}},
Pages = {{70-75}},
Abstract = {{Australian agriculture is faced with environmental issues including
   increasing land and river salinity and reduced biodiversity. Key to the
   addressing these are to develop livestock systems that achieve improved
   environmental outcomes and increased profit, in order to promote
   widespread adoption. EverGraze is a national research and extension
   project developing innovative livestock production systems based on
   perennial pastures which will deliver improved profit and environmental
   outcomes. Integrated bioeconomic and biophysical modeling (in
   consultation with farmers and catchment managers) was used to identify
   farming systems which have the potential to deliver improved profit and
   environmental outcomes, as well as identifying constraints to achieving
   these outcomes. Whole farm systems and component research is now
   underway across several sites in the high rainfall zones (>550 mm) of
   southern Australia to test the potential of new livestock systems and to
   investigate the specific benefits of parts of these systems (e.g.,
   improving sheep reproductive performance through perennial pastures and
   shrubs). This paper presents modelling results and how they were used to
   design field experiments, and preliminary experimental results from one
