@article{ ISI:000240253000016,
Author = {Renne, Ian J. and Tracy, Benjamin F. and Colonna, Ignacio A.},
Title = {{Shifts in grassland invasibility: Effects of soil resources,
   disturbance, composition, and invader size}},
Journal = {{ECOLOGY}},
Year = {{2006}},
Volume = {{87}},
