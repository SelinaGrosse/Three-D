@article{ ISI:000240244100009,
Author = {Hopfensperger, Kristine N. and Wu, Joan Q. and Gill, Richard A.},
Title = {{Plant composition and erosion potential of a grazed wetland in the
   Salmon River Subbasin, Idaho}},
Journal = {{WESTERN NORTH AMERICAN NATURALIST}},
