@article{ ISI:000240571100012,
Author = {Branson, David H. and Joern, Anthony and Sword, Gregory A.},
Title = {{Sustainable management of insect herbivores in grassland ecosystems: New
   perspectives in grasshopper control}},
Journal = {{BIOSCIENCE}},
