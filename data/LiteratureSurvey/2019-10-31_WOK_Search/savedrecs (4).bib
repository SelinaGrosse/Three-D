@article{ ISI:000296998200003,
Author = {Puyravaud, Jean-Philippe and Mohandass, D. and Davidar, Priya},
Title = {{Impact of human-related disturbance on Eriochrysis rangacharii Fischer,
   a rare keystone endemic grass (Nilgiris, southern India): a preliminary
   assessment}},
Journal = {{TROPICAL ECOLOGY}},
Year = {{2012}},
Volume = {{53}},
Number = {{1}},
Pages = {{25-32}},
Month = {{JAN}},
Abstract = {{We assessed the factors influencing the perceived decline of Eriochrysis
   rangacharii, a rare endemic swamp grass species of the Nilgiri plateau
   (Western Ghats of India), which had been listed as ``presumed
   extinct{''} for over 100 years in the Red Data Book of Indian Plants.
   This grass is used by the Todas, a local ethnic community, to thatch
