@article{ ISI:000220913000009,
Author = {Sarmiento, G and Pinillos, M and da Silva, MP and Acevedo, D},
Title = {{Effects of soil water regime and grazing on vegetation diversity and
   production in a hyperseasonal savanna in the Apure Llanos, Venezuela}},
Journal = {{JOURNAL OF TROPICAL ECOLOGY}},
Year = {{2004}},
Volume = {{20}},
Number = {{2}},
Pages = {{209-220}},
Month = {{MAR}},
Abstract = {{Soil water content and above-ground biomass accumulation, above 10 cm
   high, were measured monthly in a flooded savanna ecosystem under grazing
   pressure and under cattle exclusion, during two growth cycles.
   Near-to-the-ground and below-ground biomass were measured three times
   during this period. Besides, composition, species richness and diversity
   were obtained through a floristic inventory. Despite a relatively high
   floristic richness and diversity, Panicum laxum is the dominant species
   throughout the study area, while three other perennial grasses, Paspalum
   chaffanjonii, Leersia hexandra and Axonopus purpusii, also reach high
   values of cover and biomass. Each of them reacts specifically to
   flooding, drought and grazing conditions. This ecosystem shows a
   strongly seasonal behaviour. with primary production, mortality and
   decomposition sharply timed by soil relative water content. Both drought
   and water excess seem to limit plant production, even more during wet
   years when the savanna might remain flooded for up to 4 mo. Some
   structural and functional differences between the grazed and the
   protected systems are demonstrated. but under the actual, relatively low
   stocking rate, the grazed savanna produces as much forage as the
   ungrazed one.}},
