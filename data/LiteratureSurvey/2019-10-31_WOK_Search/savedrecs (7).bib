DOI = {{10.1046/j.1461-0248.2002.00293.x}},
ISSN = {{1461-023X}},
Keywords = {{ANPP; community dynamics; disturbance; drought; litter; long-term
   research; nitrogen; oscillations; primary production; synchronous
