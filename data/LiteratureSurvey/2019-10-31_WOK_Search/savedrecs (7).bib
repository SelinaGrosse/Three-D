Address = {{9600 GARSINGTON RD, OXFORD OX4 2DG, OXON, ENGLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Rudgers, JA (Reprint Author), Indiana Univ, Dept Biol, Bloomington, IN 47405 USA.
   Indiana Univ, Dept Biol, Bloomington, IN 47405 USA.}},
