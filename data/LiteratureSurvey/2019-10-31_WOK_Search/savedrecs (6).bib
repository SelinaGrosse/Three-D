@article{ ISI:000255168700007,
Author = {Sporndly, Eva and Widen, Olga},
Title = {{Grazing semi-natural pastures late in the season or every second year
   effects on the weight gain of steers and composition of selected
   vegetation}},
Journal = {{ACTA AGRICULTURAE SCANDINAVICA SECTION A-ANIMAL SCIENCE}},
Year = {{2007}},
Volume = {{57}},
Number = {{3}},
Pages = {{159-172}},
Abstract = {{In a five year experiment the weight gain of a total of 106 steers on
   three grazing treatments was studied: Partial-Delay (delayed onset of
   grazing in part of the grazing area), Intermittent (grazing every second
   year) and Continuous (continuous yearly grazing). Daily weight gain of
   steers was similar between treatments during the first three years but
   lower (P < 0.05) on the Partial-Delay (0.6 kg) compared with the
   Intermittent and Continuous treatments (0.8-0.9 kg) during the last 2
   experimental years. The mean proportions of grass, forbs and senescent
   plant material were similar between treatments: 59-61, 23-24, 15-17\% of
   dry matter (DM), respectively.}},
