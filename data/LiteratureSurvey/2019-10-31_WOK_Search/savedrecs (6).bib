@article{ ISI:000249500900023,
Author = {Sanders, Nathan J. and Weltzin, Jake F. and Crutsinger, Gregory M. and
   Fitzpatrick, Matthew C. and Nunez, Martin A. and Oswalt, Christopher M.
   and Lane, Kristine E.},
Title = {{Insects mediate the effects of propagule supply and resource
   availability on a plant invasion}},
Journal = {{ECOLOGY}},
Year = {{2007}},
Volume = {{88}},
Number = {{9}},
Pages = {{2383-2391}},
Month = {{SEP}},
Abstract = {{Invasive species are a global threat to biodiversity and the functioning
   of natural ecosystems. Here, we report on a two-year experiment aimed at
   elucidating the combined and relative effects of three key controls on
   plant invasions: propagule supply, soil nitrogen (N) availability, and
   herbivory by native insects. We focus on the exotic species Lespedeza
   cuneata, a Rank I invasive species. Propagule supply and soil
   N-availability interacted to control the density and foliar cover of L.
   cuneata. In low N plots, density and foliar cover of L. cuneata were
   higher in the propagule addition plots than in the plots to which
   propagules were not added. Surprisingly, this interaction was
   significant only when the abundance of herbivores was experimentally
   reduced. This experiment provides evidence that native insect herbivores
   mediate the interactive effects of propagule supply and resources on
