@article{ ISI:000257528100013,
Author = {Harvey, Jeffrey A. and van der Putten, Wim H. and Turin, Hans and
   Wagenaar, Roel and Bezemer, T. Martijn},
Title = {{Effects of changes in plant species richness and community traits on
   carabid assemblages and feeding guilds}},
