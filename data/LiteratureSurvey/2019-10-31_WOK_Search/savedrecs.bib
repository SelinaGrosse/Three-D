@article{ ISI:000452555800007,
Author = {Bajard, Manon and Etienne, David and Quinsac, Sebastien and Dambrine,
   Etienne and Sabatier, Pierre and Frossard, Victor and Gaillard, Jeremie
   and Develle, Anne-Lise and Poulenard, Jerome and Arnaud, Fabien and
