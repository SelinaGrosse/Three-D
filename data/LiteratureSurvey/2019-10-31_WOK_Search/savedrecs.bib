Type = {{Article}},
Language = {{English}},
Affiliation = {{Evans, MJ (Reprint Author), Australian Natl Univ, Fenner Sch Environm \& Soc, Canberra, ACT 0200, Australia.
   Evans, Maldwyn J.; Cunningham, Saul A.; Manning, Adrian D.; Barton, Philip S., Australian Natl Univ, Fenner Sch Environm \& Soc, Canberra, ACT 0200, Australia.
   Gibb, Heloise, La Trobe Univ, Dept Ecol Environm \& Evolut, Melbourne, Vic 3086, Australia.}},
DOI = {{10.1016/j.ecolind.2019.05.005}},
