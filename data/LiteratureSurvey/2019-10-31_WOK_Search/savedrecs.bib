Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
Type = {{Article; Early Access}},
Language = {{English}},
Affiliation = {{Buse, J (Reprint Author), Black Forest Natl Pk, Kniebisstr 67, D-72250 Freudenstadt, Germany.
   Buse, Joern, Black Forest Natl Pk, Kniebisstr 67, D-72250 Freudenstadt, Germany.
   Buse, Joern; Entling, Martin H., Univ Koblenz Landau, iES Landau Inst Environm Sci, Landau, Germany.}},
