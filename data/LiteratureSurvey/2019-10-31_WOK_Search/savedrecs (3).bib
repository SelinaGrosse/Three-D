@article{ ISI:000324110200030,
Author = {Damgaard, Christian and Thomsen, Marie Paarup and Borchsenius, Finn and
   Nielsen, Knud Erik and Strandberg, Morten},
Title = {{The effect of grazing on biodiversity in coastal dune heathlands}},
Journal = {{JOURNAL OF COASTAL CONSERVATION}},
Year = {{2013}},
Volume = {{17}},
Number = {{3}},
Pages = {{663-670}},
Month = {{SEP}},
Abstract = {{The effect of sheep grazing on species richness, higher order diversity
   measures, inequality, species composition, functional diversity and
   allometric relationships at a coastal dune heathland site was
   investigated. After a prescribed fire in 2002, the site was divided into
   two parts, where one of the parts was unmanaged and the other part was
   fenced and grazed by sheep. Sheep grazing had a positive effect on
   species richness as well as a significant positive effect on the
   functional diversity at the coastal dune heathland site. Generally, the
   cover of dwarf shrubs was negatively affected by grazing, whereas the
   cover of sedges and grasses was positively affected by grazing. There is
   a need for comparative investigations of the effect of different
   management methods not only on floristic biodiversity, but on all
   relevant kinds of biodiversity as well as on soil structure, soil
   chemistry and habitat micro- and macrostructure. Consequently, we
   advocate the initiation of an international systematic investigation of
   the effect of different management methods.}},
