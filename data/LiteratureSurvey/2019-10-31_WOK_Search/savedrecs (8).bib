@article{ ISI:A1996VK17800009,
Author = {Watt, TA and Treweek, JR and Woolmer, FS},
Title = {{An experimental study of the impact of seasonal sheep grazing on
   formerly fertilized grassland}},
Journal = {{JOURNAL OF VEGETATION SCIENCE}},
Year = {{1996}},
Volume = {{7}},
Number = {{4}},
Pages = {{535-542}},
Month = {{AUG}},
Abstract = {{The effects of grazing upon the composition of a grassland were examined
   in a replicated grazing experiment with sheep over eight years. Grazing
   was controlled to produce two levels of grazing in each of winter,
   spring and summer, combined in a 2 x 2 x 2 factorial experimental
   design. Particular species were either increased or decreased by winter
   grazing while, in general, dicotyledonous species were favoured by
   grazing in both winter and spring. A model fitting the main effects of
   grazing in three seasons showed the importance of grazing in winter and
   spring to achieve sward diversification.}},
