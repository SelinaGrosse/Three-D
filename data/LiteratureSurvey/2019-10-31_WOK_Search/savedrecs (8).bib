@article{ ISI:000084868000011,
Author = {Homewood, K and Brockington, D},
Title = {{Biodiversity, conservation and development in Mkomazi Game Reserve,
   Tanzania}},
Journal = {{GLOBAL ECOLOGY AND BIOGEOGRAPHY}},
Year = {{1999}},
Volume = {{8}},
Number = {{3-4}},
